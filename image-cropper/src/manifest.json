{
  "manifest_version": 3,
  "name": "Image Cropper",
  "description": "Crop images before downloading them locally.",
  "version": "0.1.0",
  "action": {
    "default_title": "Image Cropper"
  },
  "permissions": ["activeTab", "downloads", "scripting"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
